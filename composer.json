{
  "name":              "Laravel Scaffolding",
  "description":       "Scaffolding for new laravel app with must have packages and configurations.",
  "keywords":          [
    "framework",
    "laravel",
    "scaffolding",
    "boiler-plate"
  ],
  "license":           "MIT",
  "type":              "project",
  "require":           {
    "php": "^7.1.3",
    "doctrine/dbal": "^2.7",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.6.*",
    "laravel/tinker": "^1.0",
    "league/fractal": "^0.17.0",
    "prettus/l5-repository": "^2.6",
    "spatie/laravel-activitylog": "^2.5",
    "spatie/laravel-cors": "^1.2",
    "spatie/laravel-permission": "^2.11",
    "webpatser/laravel-uuid": "^3.0"
  },
  "require-dev":       {
    "barryvdh/laravel-debugbar":   "^3.1",
    "barryvdh/laravel-ide-helper": "^2.4",
    "filp/whoops":                 "^2.0",
    "fzaninotto/faker":            "^1.4",
    "mockery/mockery":             "^1.0",
    "nunomaduro/collision":        "^2.0",
    "phpunit/phpunit":             "^7.0"
  },
  "autoload":          {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4":    {
      "App\\": "app/"
    },
    "files":    [
      "app/Infrastructure/Helpers/common.php",
      "app/Infrastructure/Helpers/auth.php"
    ]
  },
  "autoload-dev":      {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra":             {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts":           {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd":   [
      "@php artisan key:generate"
    ],
    "post-autoload-dump":        [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config":            {
    "preferred-install":   "dist",
    "sort-packages":       true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable":     true
}
